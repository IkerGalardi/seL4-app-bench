<?xml version="1.0" encoding="UTF-8"?>
<system>
    <!-- Memory mapped registers of the PL011 uart device -->
    <memory_region name="pl011_regs" size="0x1000" phys_addr="0x9000000" />

    <protection_domain name="seriald">
        <program_image path="build/seriald.elf"/>

        <map mr="pl011_regs" vaddr="0x4000000" perms="rw" cached="false" setvar_vaddr="pl011_base_vaddr" />
        <irq id="0" irq="33"/>
    </protection_domain>
</system>
